	<parameter name="letra_comp" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT so.nombre AS SumiNomPos,
su.calle_postal AS Sumi_calpo,
su.nro_postal AS Sumi_ncalp,
su.piso_postal AS Sumi_ppost,
su.depto_postal AS Sumi_dpost,
su.CP_postal AS Sumi_popos,
su.Localidad_postal AS Sumi_lopos,
so.nombre AS Soc_apelli,
su.calle AS Sumi_calle,
su.nro AS Sumi_nroca,
su.piso AS Sumi_piso,
su.depto AS Sumi_depto,
fa.nro_socio AS Soc_numero,
fa.nro_suministro AS Sumi_numer,
fa.nro_lote AS FactNroLo,
fa.comp_letra AS Fact_letra,
comp_tipo, comp_sucur, comp_nro,
concat(comp_letra, '-' , lpad(comp_sucur, 4, '0'), '-',
	lpad(comp_nro, 8, '0'))  AS Fact_numer,
importe AS Monto,
date(fecha_vto) AS Fact_fecvt,
date(fecha_vto_proximo) AS FactProVto,
date(fecha_emision) AS Fact_fecha,
date(fecha_lectura_anterior) AS fecha_lectura_anterior,
date(fecha_lectura_actual) AS fecha_lectura_actual,
concat(lsp_letra, '-' , lpad(lsp_sucursal, 4, '0'), '-',
	lpad(lsp_numero, 8, '0')) AS lsp_numer,
su.IVA as iva,
so.cuit as cuit,
SUBSTRING_INDEX(cat.descripcion, '-', -1) as categ,
fa.area_ruta_orden as area,
concat(fa.tipo_medidor , '-', fa.medidor) as medidor,
round(lectura_anterior) as lectura_anterior,
round(lectura_actual) as lectura_actual,
round(lectura_actual - lectura_anterior) as consumo_real,
concat('296', lpad(fa.nro_socio, 6, '0'), lpad(fa.nro_suministro, 6, '0'), '0000') as pago_elec,
concat(fa.nro_socio, lpad(fa.nro_suministro, 3, '0')) as id_sumi,
codigo_barras,
concat('03', if(fa.comp_letra = 'A', 1, 2), lpad(fa.comp_sucur, 4, '0'), lpad(fa.comp_nro, 8, '0')) as codigo_barras_soc,
fecha_pago_banco as fecha_pago_banco,
cesp as cesp,
date(cesp_fecha) as cesp_fecha,
partida as partida,
padron as padron,
anticipo as anticipo,
ano_consumo as año,
mes_consumo as mes,
date(desde_ant) as desde_ant,
cf_ant, cv_ant, kw_ant, dias_ant, lista_ant,
date(desde_act) as desde_act,
cf_act, cv_act, kw_act, dias_act, lista_act,
fa.id_upc,
descri,
factCAEnro, factCAEvto

from  t_facturacion fa left join t_suministro su on  fa.nro_socio = su.nro_socio
		and fa.nro_suministro = su.nro_suministro
	left join t_socio so on fa.nro_socio = so.nro_socio
	left join t_suministro_categ cat on fa.categoria = cat.id

where
	fa.comp_tipo = $P{tipo_comp_id}
	and fa.comp_sucur = $P{prefijo_comp}
	and fa.comp_nro = $P{sufijo_comp}
	and fa.comp_letra = $P{letra_comp}]]>
	</queryString>
